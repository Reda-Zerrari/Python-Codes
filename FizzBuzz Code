n=int(input("enter the max"))
for i in range(1,n+1):
    while True:
        if i%3==0  and i%5==0 :
            print("fizzbuzz")
            break
        if i%3==0 :
            print("fizz")
            break
        if i%5==0 :
            print("buzz")
            break
        else :
            print(i)
            break
